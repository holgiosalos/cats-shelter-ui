{
  "consumer": {
    "name": "CatShelterFront"
  },
  "interactions": [
    {
      "description": "a request to list all animals",
      "providerState": "has animals",
      "request": {
        "method": "GET",
        "path": "/animals"
      },
      "response": {
        "body": [
          {
            "breed": "Bengali",
            "gender": "Female",
            "name": "manchas",
            "vaccinated": true
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type",
            "min": 1
          },
          "$.body[*].breed": {
            "match": "type"
          },
          "$.body[*].gender": {
            "match": "type"
          },
          "$.body[*].name": {
            "match": "type"
          },
          "$.body[*].vaccinated": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to register a cat",
      "providerState": "there are no animals",
      "request": {
        "body": {
          "breed": "Bengali",
          "gender": "Female",
          "name": "Manchas",
          "vaccinated": true
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.breed": {
            "match": "type"
          },
          "$.body.gender": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.vaccinated": {
            "match": "type"
          }
        },
        "method": "POST",
        "path": "/animals"
      },
      "response": {
        "body": {
          "breed": "Bengali",
          "gender": "Female",
          "name": "Manchas",
          "vaccinated": true
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.breed": {
            "match": "type"
          },
          "$.body.gender": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          },
          "$.body.vaccinated": {
            "match": "type"
          }
        },
        "status": 201
      }
    }

  ],
  "metadata": {
    "pact-js": {
      "version": "11.0.2"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "CatShelterBack"
  }
}